{
    "name": "phpnomad/core",
    "description": "",
    "type": "library",
    "homepage": "https://github.com/phpnomad/core",
    "readme": "README.md",
    "license": "MIT",
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/tests.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/di.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/cache.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/db.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/loader.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/rest.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/event.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/facade.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/singleton.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/utils.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpnomad/logger.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "PHPNomad\\Core\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPNomad\\Core\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alex Standiford",
            "email": "alex@standiford.us"
        }
    ],
    "require-dev": {
        "phpnomad/tests": "^0.1.0"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "require": {
        "phpnomad/utils": "dev-main",
        "phpnomad/event": "dev-main",
        "phpnomad/cache": "dev-main",
        "phpnomad/di": "dev-main",
        "phpnomad/rest": "dev-main",
        "phpnomad/singleton": "dev-main",
        "phpnomad/facade": "dev-main",
        "phpnomad/loader": "dev-main",
        "phpnomad/logger": "dev-main",
        "ext-json": "*"
    }
}
